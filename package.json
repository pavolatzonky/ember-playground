  "private": true,
  "scripts": {
    "ember": "ember",
    "build": "ember build",
    "start": "FASTBOOT_DISABLED=true ember serve",
    "pretest": "rm -rf coverage/",
    "test": "COVERAGE=true ember test --silent -r xunit",
    "test:browser": "EMBER_DEBUG=true ember test --server",
    "test:exam": "COVERAGE=true ember exam --parallel=1",
    "try:each": "ember try:each",
    "coverage": "ember coverage-merge && rm -rf ./coverage_* && cat coverage/lcov.info",
    "lint": "npm-run-all --parallel lint:**",
    "lint:hbs": "DISABLE_GITHUB_ACTIONS_ANNOTATIONS=true ember-template-lint --quiet app",
    "lint:js": "eslint ./*.js ./.*.js app aws fastboot headers config lib mirage server sitemap tests tests-fastboot tests-node",
    "lint:css": "stylelint 'app/**/*.css'",
    "lint:scss": "stylelint 'app/**/*.scss' --config .stylelintrc.scss.js",
    "lint:translations": "node lib/utils/check-for-missing-translation-files.js"
  },
  "homepage": "https://github.com/pavolatzonky/ember-playground#readme",
