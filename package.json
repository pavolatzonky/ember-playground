{
  "name": "ember-playground",
  "version": "0.0.1",
  "description": "A bare EmberJS application to play with",
  "repository": {
    "type": "git",
    "url": "git@github.com:pavolatzonky/ember-playground.git"
  },
  "license": "UNLICENSED",
  "directories": {
    "test": "tests"
